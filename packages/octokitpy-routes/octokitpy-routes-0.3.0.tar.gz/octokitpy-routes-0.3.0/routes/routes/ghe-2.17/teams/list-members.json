{
  "name": "List team members",
  "enabledForApps": true,
  "method": "GET",
  "path": "/teams/:team_id/members",
  "previews": [
    {
      "name": "hellcat",
      "description": "**Note:** The Nested Teams API is currently available for developers to preview. See the [blog post](https://developer.github.com/changes/2017-08-30-preview-nested-teams) for full details. To access the API, you must provide a custom [media type](https://developer.github.com/enterprise/2.17/v3/media) in the `Accept` header:\n\n```\napplication/vnd.github.hellcat-preview+json\n\n```",
      "required": false
    }
  ],
  "params": [
    {
      "name": "team_id",
      "type": "integer",
      "required": true,
      "description": "",
      "location": "url"
    },
    {
      "name": "role",
      "type": "string",
      "description": "Filters members returned by their role in the team. Can be one of:  \n\\* `member` - normal members of the team.  \n\\* `maintainer` - team maintainers.  \n\\* `all` - all members of the team.",
      "default": "all",
      "required": false,
      "enum": [
        "member",
        "maintainer",
        "all"
      ],
      "location": "query"
    },
    {
      "name": "per_page",
      "type": "integer",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30,
      "location": "query"
    },
    {
      "name": "page",
      "type": "integer",
      "required": false,
      "description": "Page number of the results to fetch.",
      "default": 1,
      "location": "query"
    }
  ],
  "description": "If you pass the `hellcat-preview` media type, team members will include the members of child teams.\n\nTo list members in a team, the team must be visible to the authenticated user.",
  "responses": [
    {
      "headers": {
        "status": "200 OK",
        "content-type": "application/json; charset=utf-8"
      },
