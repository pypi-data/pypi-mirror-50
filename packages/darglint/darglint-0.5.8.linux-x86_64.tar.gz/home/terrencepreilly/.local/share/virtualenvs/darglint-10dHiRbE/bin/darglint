#!/home/terrencepreilly/.local/share/virtualenvs/darglint-10dHiRbE/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'darglint==0.5.8','console_scripts','darglint'
__requires__ = 'darglint==0.5.8'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('darglint==0.5.8', 'console_scripts', 'darglint')()
    )
