server {
        server_name server.kevo.online;

        location /static/ {
                root /home/kevin/LibAppServ;
        }

        location / {
                include proxy_params;
                proxy_pass http://unix:/home/kevin/LibAppServ/LibAppServ.sock;
        }

}

server {
    if ($host = server.kevo.online) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        listen 80;
        listen [::]:80;
        server_name server.kevo.online;
    return 404; # managed by Certbot
}