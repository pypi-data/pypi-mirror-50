server {
        server_name server.kevo.online;

        location /static/ {
                root /home/kevin/LibAppServ;
        }

        location / {
                include proxy_params;
                proxy_pass http://unix:/home/kevin/LibAppServ/LibAppServ.sock;
        }


    listen [::]:443 ssl ipv6only=on; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/server.kevo.online/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/server.kevo.online/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot



}


server {
    if ($host = server.kevo.online) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        listen 80;
        listen [::]:80;
        server_name server.kevo.online;
    return 404; # managed by Certbot
}