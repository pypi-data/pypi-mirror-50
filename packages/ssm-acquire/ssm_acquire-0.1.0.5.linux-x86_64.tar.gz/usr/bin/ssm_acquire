#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ssm-acquire==0.1.0.5','console_scripts','ssm_acquire'
__requires__ = 'ssm-acquire==0.1.0.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ssm-acquire==0.1.0.5', 'console_scripts', 'ssm_acquire')()
    )
