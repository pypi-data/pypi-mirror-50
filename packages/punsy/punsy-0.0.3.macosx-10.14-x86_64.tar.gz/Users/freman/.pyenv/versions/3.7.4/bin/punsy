#!/Users/freman/.pyenv/versions/3.7.4/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'punsy==0.0.3','console_scripts','punsy'
__requires__ = 'punsy==0.0.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('punsy==0.0.3', 'console_scripts', 'punsy')()
    )
