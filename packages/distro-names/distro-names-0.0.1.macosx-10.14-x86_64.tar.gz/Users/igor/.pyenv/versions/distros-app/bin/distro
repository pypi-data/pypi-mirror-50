#!/Users/igor/.pyenv/versions/distros-app/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'distro-names==0.0.1','console_scripts','distro'
__requires__ = 'distro-names==0.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('distro-names==0.0.1', 'console_scripts', 'distro')()
    )
