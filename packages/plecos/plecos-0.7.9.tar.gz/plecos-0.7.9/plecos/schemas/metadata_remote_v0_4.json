{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "main",
    "curation"
  ],
  "properties": {
    "main": {
      "$id": "#/properties/main",
      "type": "object",
      "title": "The Main Schema",
      "default": null,
      "additionalProperties": false,
      "required": [
        "name",
        "dateCreated",
        "datePublished",
        "author",
        "license",
        "price",
        "files",
        "encryptedFiles",
        "checksum"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/main/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "10 Monkey Species Small"
          ],
          "pattern": "^(.*)$"
        },
        "dateCreated": {
          "$id": "#/properties/main/properties/dateCreated",
          "title": "The Datecreated Schema",
          "default": "",
          "examples": [
            "2012-02-01T10:55:11Z"
          ],
          "pattern": "^(.*)$"
        },
        "datePublished": {
          "$id": "#/properties/main/properties/datePublished",
          "title": "The Datepublished Schema",
          "default": "",
          "examples": [
            "2012-02-01T10:55:11Z"
          ],
          "pattern": "^(.*)$"
        },
        "author": {
          "$id": "#/properties/main/properties/author",
          "type": "string",
          "title": "The Author Schema",
          "default": "",
          "examples": [
            "Mario"
          ],
          "pattern": "^(.*)$"
        },
        "license": {
          "$id": "#/properties/main/properties/license",
          "type": "string",
          "title": "The License Schema",
          "default": "",
          "examples": [
            "CC0: Public Domain"
          ],
          "pattern": "^(.*)$"
        },
        "price": {
          "$id": "#/properties/main/properties/price",
          "type": "string",
          "title": "The Price Schema",
          "default": "0",
          "examples": [
            "10"
          ],
          "pattern": "^[0-9]+$"
        },
        "files": {
          "$id": "#/properties/main/properties/files",
          "type": "array",
          "title": "The Files Schema",
          "default": null,
          "additionalItems": false,
          "items": {
            "$id": "#/properties/main/properties/files/items",
            "type": "object",
            "title": "The Items Schema",
            "default": null,
            "additionalProperties": false,
            "required": [
              "index",
              "contentType"
            ],
            "properties": {
              "index": {
                "$id": "#/properties/main/properties/files/items/properties/index",
                "type": "integer",
                "title": "The Index Schema",
                "default": 0,
                "examples": [
                  0
                ]
              },
              "contentType": {
                "$id": "#/properties/main/properties/files/items/properties/contentType",
                "type": "string",
                "title": "The Contenttype Schema",
                "default": "",
                "examples": [
                  "application/zip"
                ],
                "pattern": "^(.*)$"
              },
              "encoding": {
                "$id": "#/properties/main/properties/files/items/properties/encoding",
                "type": "string",
                "title": "The Encoding Schema",
                "default": "",
                "examples": [
                  "UTF-8"
                ],
                "pattern": "^(.*)$"
              },
              "compression": {
                "$id": "#/properties/main/properties/files/items/properties/compression",
                "type": "string",
                "title": "The Compression Schema",
                "default": "",
                "examples": [
                  "zip"
                ],
                "pattern": "^(.*)$"
              },
              "checksum": {
                "$id": "#/properties/main/properties/files/items/properties/checksum",
                "type": "string",
                "title": "The Checksum Schema",
                "default": "",
                "examples": [
                  "2bf9d229d110d1976cdf85e9f3256c7f"
                ],
                "pattern": "^(.*)$"
              },
              "checksumType": {
                "$id": "#/properties/main/properties/files/items/properties/checksumType",
                "type": "string",
                "title": "The Checksumtype Schema",
                "default": "",
                "examples": [
                  "MD5"
                ],
                "pattern": "^(.*)$"
              },
              "resourceId": {
                "$id": "#/properties/main/properties/files/items/properties/resourceId",
                "type": "string",
                "title": "The resourceId Schema",
                "default": 0,
                "examples": [
                  "bucket:47550"
                ]
              },
              "contentLength": {
                "$id": "#/properties/main/properties/files/items/properties/contentLength",
                "type": "string",
                "title": "The Contentlength Schema",
                "default": 0,
                "examples": [
                  "12057507"
                ]
              }
            }
          }
        },
        "encryptedFiles": {
          "$id": "#/properties/main/properties/encryptedFiles",
          "type": "string",
          "title": "The Encryptedfiles Schema",
          "default": "",
          "examples": [
            "234ab87234acbd095430853424ab87234acbd09543085340abffh21983ddhiiee9821438274234210abffh21983ddhiiee982143827423421"
          ],
          "pattern": "^(.*)$"
        },
        "checksum": {
          "$id": "#/properties/main/properties/checksum",
          "type": "string",
          "title": "The Checksum Schema",
          "default": "",
          "examples": [
            ""
          ],
          "pattern": "^0x[a-fA-F0-9]{64}$"
        },
        "categories": {
          "$id": "#/properties/main/properties/categories",
          "type": "array",
          "title": "The Categories Schema",
          "default": null,
          "additionalItems": false,
          "items": {
            "$id": "#/properties/main/properties/categories/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "image"
            ],
            "pattern": "^(.*)$"
          }
        },
        "tags": {
          "$id": "#/properties/main/properties/tags",
          "type": "array",
          "title": "The Tags Schema",
          "default": null,
          "additionalItems": false,
          "items": {
            "$id": "#/properties/main/properties/tags/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "image data",
              " animals"
            ],
            "pattern": "^(.*)$"
          }
        },
        "type": {
          "$id": "#/properties/main/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "dataset"
          ],
          "pattern": "^(.*)$"
        },
        "description": {
          "$id": "#/properties/main/properties/description",
          "type": "string",
          "title": "The Description Schema",
          "default": "",
          "examples": [
            "EXAMPLE ONLY "
          ],
          "pattern": "^(.*)$"
        },
        "copyrightHolder": {
          "$id": "#/properties/main/properties/copyrightHolder",
          "type": "string",
          "title": "The Copyrightholder Schema",
          "default": "",
          "examples": [
            "Unknown"
          ],
          "pattern": "^(.*)$"
        },
        "workExample": {
          "$id": "#/properties/main/properties/workExample",
          "type": "string",
          "title": "The Workexample Schema",
          "default": "",
          "examples": [
            "image path, id, label"
          ],
          "pattern": "^(.*)$"
        },
        "links": {
          "$id": "#/properties/main/properties/links",
          "type": "array",
          "title": "The Links Schema",
          "default": null,
          "additionalItems": false,
          "items": {
            "$id": "#/properties/main/properties/links/items",
            "type": "object",
            "title": "The Items Schema",
            "default": null,
            "additionalProperties": true,
            "required": [
              "url"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/main/properties/links/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "example model"
                ],
                "pattern": "^(.*)$"
              },
              "type": {
                "$id": "#/properties/main/properties/links/items/properties/type",
                "type": "string",
                "title": "sample link",
                "default": "",
                "examples": [
                  "https://sample.com"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/main/properties/links/items/properties/url",
                "type": "string",
                "title": "The Url Schema",
                "default": "",
                "examples": [
                  "https://drive.google.com/open?id=1uuz50RGiAW8YxRcWeQVgQglZpyAebgSM"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "inLanguage": {
          "$id": "#/properties/main/properties/inLanguage",
          "type": "string",
          "title": "The Inlanguage Schema",
          "default": "",
          "examples": [
            "en"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "additionalInformation": {
          "$id": "#/properties/main/properties/additionalInformation",
          "type": "object",
          "title": "The Additionalinformation Schema",
          "default": null,
          "properties": {
            "info": {
              "$id": "#/properties/main/properties/additionalInformation/properties/info",
              "type": "string",
              "title": "The Info Schema",
              "default": "",
              "examples": [
                "extra information objects"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
    "curation": {
          "$id": "#/properties/main/properties/curation",
          "type": "object",
          "title": "The Curation Schema",
          "default": null,
          "additionalProperties": true,
          "required": [
            "rating",
            "numVotes"
          ],
          "properties": {
            "rating": {
              "$id": "#/properties/main/properties/curation/properties/rating",
              "type": "number",
              "title": "The Rating Schema",
              "default": 0.0,
              "examples": [
                0.93
              ]
            },
            "numVotes": {
              "$id": "#/properties/main/properties/curation/properties/numVotes",
              "type": "integer",
              "title": "The Numvotes Schema",
              "default": 0,
              "examples": [
                123
              ]
            },
            "schema": {
              "$id": "#/properties/main/properties/curation/properties/schema",
              "type": "string",
              "title": "The Schema Schema",
              "default": "",
              "examples": [
                "Binary Voting"
              ],
              "pattern": "^(.*)$"
            },
            "isListed": {
              "$id": "#/properties/main/properties/curation/properties/isListed",
              "type": "boolean",
              "title": "The Islisted Schema",
              "default": false,
              "examples": [
                true
              ]
            }
          }
        }
  }
}