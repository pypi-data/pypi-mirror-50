#!/usr/bin/env bash
args="$@"

# if you have mls but not tls_python, try rerunning setup.py
tls_python $args

# was -q in args? then call temp bash script
for var in $args; do
    if [[ $var == -* ]]; then
        if [[ $var == *"q"* ]]; then
            source ~/.treepy_temp.sh
            rm ~/.treepy_temp.sh
        fi
    fi
done
